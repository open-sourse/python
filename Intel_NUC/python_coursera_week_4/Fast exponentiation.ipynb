    "def power(a, n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    if n % 2 == 0:\n",
    "        return power(a, n/2)**2\n",
    "    else:\n",
    "        return a*power(a, n-1)\n",
    "a = int(input())\n",
    "n = int(input())\n",
    "print(power(a, n))"
   ]
