    "import math \n",
    "def reduce_fraction(n,m):\n",
    "    k = math.gcd(n,m)\n",
    "    return (n//k, m//k)\n",
    "n = int(input())\n",
    "m = int(input())\n",
    "reduce_fraction(n,m)\n"
